<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    
    <!-- Background Colors -->
    <Color x:Key="primaryBackColor1">#ffffff</Color>
    <Color x:Key="primaryBackColor2">#5efcff</Color>

    <!-- Window Border Colors -->
    <Color x:Key="winBorderColor1">#f7c51e</Color>
    <Color x:Key="winBorderColor2">#d1201d</Color>
    <Color x:Key="winBorderColor3">#f7d04d</Color>

    <!-- Color Palette -->
    <SolidColorBrush x:Key="color1">#784DFD</SolidColorBrush>
    <SolidColorBrush x:Key="color2">#bf39a0</SolidColorBrush>
    <SolidColorBrush x:Key="color3">#ab7d09</SolidColorBrush>
    <SolidColorBrush x:Key="color4">#FB539B</SolidColorBrush>
    
    
    <!-- Font Colors -->
    <SolidColorBrush x:Key="titleColor">#143d5c</SolidColorBrush>
    <SolidColorBrush x:Key="plainTextColor1">#9497CD</SolidColorBrush>
    <SolidColorBrush x:Key="plainTextColor2">#7C80C2</SolidColorBrush>
    <SolidColorBrush x:Key="plainTextColor3" Color = "#7376BD"/>
    
    <!-- Panel Colors -->
    <SolidColorBrush x:Key="panelOverColor" Color = "#64faf5"/>
    <SolidColorBrush x:Key="panelActiveColor" Color = "#a2f5ed"/>
    
</ResourceDictionary>